<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shellming.dao.OrderDao">
    <resultMap id="flower" type="Flower">
        <result property="flowerId" column="flowerId"/>
        <result property="disPrice" column="disPrice"/>
    </resultMap>

    <parameterMap id="tt" type="Order">
        <parameter property="flower" resultMap="flower"/>
    </parameterMap>
    <insert id="buyFlower" parameterMap="tt">
  INSERT into `order`(orderId, openId, flowerId, disPrice, flowerNum) VALUES (#{orderId}, #{openId}, #{flowerId}, #{disPrice}, #{flowerNum})

    </insert>

</mapper>